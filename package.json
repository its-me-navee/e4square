{
  "name": "e4square",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "start-server": "cd server && npm start",
    "start-client": "cd client && npm start",
    "dev": "concurrently \"npm run start-server\" \"npm run start-client\"",
    "install-all": "npm install && cd client && npm install && cd ../server && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
